[global_config]
  dbus = False
  handle_size = 0
  inactive_color_offset = 1.0
  enabled_plugins = ,
[keybindings]
  go_up = <Super>Up
  go_down = <Super>Down
  go_left = <Super>Left
  go_right = <Super>Right
  split_vert = <Primary><Shift>e
  hide_window = <Primary><Shift><Alt>v
  group_all = ""
  broadcast_off = <Shift><Super>g
  broadcast_group = <Super>g
  broadcast_all = <Primary><Super>g
  layout_launcher = <Primary><Shift><Alt>l
  help = <Alt>F1
[profiles]
  [[default]]
    background_color = "#220f1c"
    background_darkness = 0.0
    cursor_blink = False
    cursor_fg_color = "#ffffdc"
    cursor_bg_color = "#1b1b1d"
    font = Monospace 11
    foreground_color = "#deddda"
    scrollbar_position = hidden
    scrollback_infinite = True
    palette = "#2e3436:#cc0000:#62bf0b:#c4a000:#397de3:#75507b:#06989a:#c0bfbc:#555753:#ef2929:#8ae234:#fce94f:#729fcf:#ad7fa8:#34e2e2:#eeeeec"
    use_system_font = False
    title_transmit_bg_color = "#1a5fb4"
[layouts]
  [[default]]
    [[[child1]]]
      parent = window0
      type = Terminal
    [[[window0]]]
      parent = ""
      type = Window
[plugins]
